{
  "name": "@sse-auth/react",
  "version": "1.4.0",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "source": "index.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.es.js",
      "types": "./dist/index.d.ts"
    },
    "./provider": {
      "require": "./dist/providers.cjs.js",
      "import": "./dist/providers.es.js",
      "types": "./dist/src/providers/index.d.ts"
    },
    "./css/*": "./dist/css/*",
    "./provider/*": "./dist/src/providers/*",
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:css": "sass --embed-sources ./src/style.scss ./dist/css/styles.css",
    "build": "rimraf dist && npm run build:css && cross-env NODE_ENV=production rollup -c",
    "prepack": "rimraf dist && npm run build:css && cross-env NODE_ENV=production rollup -c"
  },
  "author": "SSE World",
  "license": "MIT",
  "description": "A Library for Oauth Designed By SSE",
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^12.1.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.7",
    "cross-env": "^7.0.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.24.0",
    "sass": "^1.80.3",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "axios": "^1.7.7",
    "buffer": "^6.0.3",
    "jsonwebtoken": "^9.0.2",
    "react": "^18.3.1",
    "tailwind-merge": "^2.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sse-auth/react.git"
  },
  "bugs": {
    "url": "https://github.com/sse-auth/react/issues"
  },
  "homepage": "https://github.com/sse-auth/react#readme"
}
